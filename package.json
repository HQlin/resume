{
  "name": "resume",
  "version": "0.0.0",
  "description": "Own Resume",
  "main": "index.js",
  "config": {
    "port": 8000
  },
  "scripts": {
    "preserve": "run-p devcompile",
    "serve": "run-p serve:**",
    "serve:scss": "guard start",
    "serve:html": "http-server dev/$npm_package_version/ -p $npm_package_config_port",
    "serve:launch": "xdg-open http://localhost:$npm_package_config_port",
    "cptodist": "cp -r third-part dist/$npm_package_version/",
    "cptodev": "cp -r third-part dev/$npm_package_version/",
    "precompile": "rm -rf dist/$npm_package_version/ && mkdirp dist/$npm_package_version/",
    "compile": "run-p compile:**",
    "compile:html": "node javascripts/gen-html.js $npm_package_version",
    "compile:css": "compass compile && postcss -u postcss-cssnext -d dist/$npm_package_version/stylesheets stylesheets/*.css",
    "postcompile": "npm run cptodist",
    "predevcompile": "rm -rf dev/$npm_package_version/ && mkdirp dev/$npm_package_version/",
    "devcompile": "run-p devcompile:**",
    "devcompile:html": "node javascripts/gen-dev-html.js $npm_package_version",
    "devcompile:css": "postcss -u postcss-cssnext -d dev/$npm_package_version/stylesheets stylesheets/*.css",
    "postdevcompile": "npm run cptodev",
    "watch:css": "onchange \"stylesheets/**/*.css\" -- npm run devcompile:css",
    "watch:html": "onchange \"components/**/*.html\" \"./dev.html\" -- npm run devcompile:html",
    "watch": "run-p watch:**",
    "dev": "run-p watch serve",
    "build": "run-p compile",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "resume",
    "sass",
    "postcss",
    "ES6"
  ],
  "author": "JMwill",
  "license": "MIT",
  "devDependencies": {
    "art-template": "^3.1.3",
    "http-server": "^0.9.0",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.0.1",
    "onchange": "^3.2.1",
    "postcss": "^5.2.14",
    "postcss-cli": "^3.0.0-beta",
    "postcss-cssnext": "^2.9.0"
  },
  "dependencies": {}
}
